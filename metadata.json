{
  "name": "etaklar-haproxywrapper",
  "version": "0.1.0",
  "author": "Takeshi Larsson",
  "summary": "Allows usage of puppetlabs-haproxy in hiera",
  "license": "Apache 2.0",
  "source": "https://github.com/propyless/puppet-module-haproxywrapper",
